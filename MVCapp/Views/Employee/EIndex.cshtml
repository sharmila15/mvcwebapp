@model IEnumerable<BusinessLayer.Employee>
@using MVCapp.Models;
@{
    ViewBag.Title = "Index";
}
<div style="font-family:Arial">
    <h2>Employee List</h2>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                @Html.DisplayFor(modelItem => item.Name)
            </li>
        }
    </ul>
    @Html.DisplayFor(modelItem => item.City)
</div>